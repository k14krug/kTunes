{% extends 'base.html' %}

{% block content %}
    <h1>{% block title %} Welcome to kTunes index {% endblock %}</h1>
    {% for post in h_artist_cnt %}
    <a href="{{ url_for('post', artist=post['artist']) }}">
        {{ post['artist'] }} </a>
    <span class="badge badge-primary">{{ post['last_play_dt'] }}</span>
    <span class="badge badge-primary">{{ post['cnt'] }}</span>
    <hr>
{% endfor %}
{% endblock %}
